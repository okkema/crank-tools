{
  "type": "module",
  "name": "crank.tools",
  "version": "0.0.0",
  "author": {
    "name": "Okkema Labs",
    "email": "correos@okkema.org",
    "url": "https://okkema.org"
  },
  "license": "MIT",
  "scripts": {
    "build": "npm-run-all build:typescript build:vite build:storybook",
    "build:storybook": "storybook build -o dist/storybook",
    "build:typescript": "tsc",
    "build:vite": "vite build --outDir dist/vite",
    "clean": "rm -rf dist node_modules/.vite",
    "deploy:pages": "npm-run-all build",
    "lint": "eslint '**/*.{js,jsx,ts,tsx,json}'",
    "lint:fix": "eslint '**/*.{js,jsx,ts,tsx,json}' --fix",
    "lint:staged": "lint-staged",
    "precommit": "npm-run-all clean lint:staged build test",
    "preview": "vite preview",
    "prepare": "husky install",
    "start": "npm-run-all start",
    "start:vite": "vite",
    "start:storybook": "storybook dev -p 6006 --no-open",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:update": "vitest run --update"
  },
  "dependencies": {
    "@date-io/date-fns": "^2.17.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
