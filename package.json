  },
  "repository": {
    "type": "git",
    "url": "https://github.com/okkema/crank.tools"
  },
  "bugs": {
    "url": "https://github.com/okkema/crank.tools/issues"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": [
      "npm run lint:fix"
    ]
  }
}
